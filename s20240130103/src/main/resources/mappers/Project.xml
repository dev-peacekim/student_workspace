<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.oracle.oBootMybatis01.ProjectMapper">
			
	<insert id="kphProjectInsert" parameterType="Project">
		INSERT INTO PROJECT VALUES (PROJECT_SEQ.NEXTVAL, #{project_title}, #{project_content}, #{project_start}, #{project_end}, #{user_no}, SYSDATE, 0, 0)
	</insert>
	
	<select id="kphProjectList" parameterType="int" resultType="Project">
		SELECT *
		FROM PROJECT
		WHERE USER_NO = #{user_no}
	</select>
	
</mapper>